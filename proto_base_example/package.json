{
  "name": "proto-package",
  "version": "1.0.0",
  "description": "Proto package with proto definitions to import into multiple microservices",
  "main": "index.js",
  "scripts": {
    "server": "ts-node unary/server/index.ts",
    "client": "ts-node unary/client/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "unary:generate:payment:js": "grpc_tools_node_protoc --js_out=import_style=commonjs:./unary/build --grpc_out=grpc_js:./unary/build --proto_path=./unary/proto ./unary/proto/*.proto",
    "unary:generate:payment:ts": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./unary/build --proto_path=./unary/proto ./unary/proto/*.proto",
    "unary:prepare": "rimraf unary/build/*",
    "unary:build": "npm run unary:prepare && npm run unary:generate:payment:js && npm run unary:generate:payment:ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.11.1",
    "@grpc/proto-loader": "^0.7.13",
    "@types/google-protobuf": "^3.15.12",
    "google-protobuf": "^3.21.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.38.0",
    "@bufbuild/protoc-gen-es": "1.10.0",
    "@connectrpc/protoc-gen-connect-es": "^1.4.0",
    "grpc-tools": "^1.12.4",
    "ts-proto": "^2.0.3",
    "typescript": "^5.5.4"
  }
}
